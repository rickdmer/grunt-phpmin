<?php require './vendor/autoload.php'; $payload = array( "iss" => "http://example.org", "aud" => "http://example.com", "iat" => 1356999524, "nbf" => 1357000000, "exp" => time() + 30 ); $key = "example_key"; $jwt = JWT::encode( $payload, $key ); $decoded = JWT::decode( $jwt, $key, array('HS256') ); var_dump($decoded); ?>